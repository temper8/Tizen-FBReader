collections    
   images {
      image: "logo.png" COMP;
   }
   base_scale: 1.0;
   group { name: "picture_layout";
      parts {
         part { name: "bg";
            type: RECT;
            description { state: "default" 0.0;
               rel1.relative: 0.0 0.0;
               rel2.relative: 1.0 1.0;
               color: 0 125 255 128;
            }
         }
  
         part { name: "rect";
            type: RECT;
            description { state: "default" 0.0;
               rel1.relative: 0.02 0.02;
               rel2.relative: 0.48 0.7;
               color: 0 136 170 255;
            }
         }
         part { name: "group";
            type: GROUP;
            source: "my_group";
            description { state: "default" 0.0;
               rel1.relative: 0.52 0.02;
               rel2.relative: 1.0 0.16;
            }
         }
        part { name: "group2";
            type: GROUP;
            source: "my_group";
            description { state: "default" 0.0;
               rel1.relative: 0.52 0.16;
               rel2.relative: 1.0 0.28;
            }
         }
        part { name: "group3";
               type: GROUP;
               source: "my_group";
               description { state: "default" 0.0;
                  rel1.relative: 0.52 0.28;
                  rel2.relative: 1.0 0.40;
               }
         }
      }
   }
   
   group { name: "my_group";
      parts {
         part { name: "text";
            type: TEXT;
            scale: 1;
            effect: FAR_SOFT_SHADOW;
            description { state: "default" 0.0;
               color: 0 136 170 255;
               color2: 0 136 170 50;
               color3: 0 136 170 25;
               text {
                  size: 30;
                  font: "Sans";
                  text: "label";
               }
            }
         }
      }
   }
}