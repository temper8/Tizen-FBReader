collections {
   images {
      image: "logo.png" COMP;
   }
   base_scale: 1.0;
   group { name: "picture_layout";
      parts {
         part { name: "bg";
            type: RECT;
            description { state: "default" 0.0;
               rel1.relative: 0.0 0.0;
               rel2.relative: 1.0 1.0;
               color: 0 125 255 128;
            }
         }
         part { name: "image_bg";
            type: RECT;
            description { state: "default" 0.0;
               rel1.relative: 0.02 0.02;
               rel2.relative: 0.48 0.7;
               color: 0 136 170 255;
            }
         }
         part { name: "picture";
            type: IMAGE;
            description { state: "default" 0.0;
               rel1.relative: 0.02 0.02;
               rel2.relative: 0.48 0.7;
               color: 0 136 170 255;
               image.normal: "logo.png";
            }
         }
         part { name: "button1";
            type: SWALLOW;
            description { state: "default" 0.0;
               rel1.relative: 0.52 0.02;
               rel2.relative: 0.98 0.16;
            }
         }
         part { name: "button2";
            type: SWALLOW;
            description { state: "default" 0.0;
               rel1.relative: 0.52 0.18;
               rel2.relative: 1.0 0.32;
            }
         }
         part { name: "button3";
            type: SWALLOW;
            description { state: "default" 0.0;
               rel1.relative: 0.52 0.34;
               rel2.relative: 1.0 0.48;
            }
         }
         part { name: "button4";
            type: SWALLOW;
            description { state: "default" 0.0;
               rel1.relative: 0.52 0.50;
               rel2.relative: 1.0 0.64;
            }
         }
   
      }
   }
}